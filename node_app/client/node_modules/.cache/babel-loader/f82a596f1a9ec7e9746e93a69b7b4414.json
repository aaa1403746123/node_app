   <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('resgisterUser')\">注册</el-button>\r\n         </el-form-item>\r\n        </el-form>\r\n        </div>\r\n        </section>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    name:'register',\r\n    components:{},\r\n    data(){\r\n        return{\r\n            registerForm:{\r\n            name:'',\r\n            email:'',\r\n            password:'',\r\n            identify:''\r\n            },\r\n       rules:{\r\n            name:[{\r\n                reqired:true,\r\n                message:'用户名不能为空',\r\n                trigger:'blur'\r\n            },{\r\n                min:2,\r\n                max:11,\r\n                message:'长度为2到11位',\r\n                trigger:'blur'\r\n            }],\r\n            email:[{\r\n                type:\"email\",\r\n                reqired:true,\r\n                message:'邮箱格式不对',\r\n                trigger:'blur'\r\n            }],\r\n            password:[{\r\n                reqired:true,\r\n                message:'密码不能为空',\r\n                trigger:'blur'\r\n            },{\r\n                min:6,\r\n                max:18,\r\n                message:'密码长度为6到18位',\r\n                trigger:'blur'\r\n            }],\r\n            password2:[{\r\n                reqired:true,\r\n                message:'确认密码不能为空',\r\n                trigger:'blur'\r\n            }] \r\n        }};\r\n         var validatePass2 = (rule, value, callback) => {\r\n        if (value !== this.registerForm.password) {\r\n          callback(new Error('两次输入密码不一致!'));\r\n        } else {\r\n          callback();\r\n        }\r\n      };  \r\n    }, \r\n    methods:{\r\n        submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n          if (valid) {\r\n            this.$axios.post('/api/register',this.registerForm).then(res=>{\r\n                this.$message({\r\n                    message:'账号注册成功',\r\n                    type:'success'\r\n                })\r\n            })\r\n       \r\n          }\r\n        });\r\n      }\r\n    }\r\n  \r\n}\r\n</script>\r\n<style scoped>\r\n.register{\r\n               background:url(../assets/bg.jpg)no-repeat center center;\r\n               background-size: 100% 100%;\r\n               height: 100%;\r\n               position: fixed;\r\n               width: 100%\r\n}\r\n.container{\r\n    width: 370px;\r\n    height: 210px;\r\n    position: absolute;\r\n    top:10%;\r\n    left: 34%;;\r\n    text-align: center;\r\n    border-radius: 5px;\r\n}\r\n.container .manage_tip .title {\r\n  font-family: \"Microsoft YaHei\";\r\n  font-weight: bold;\r\n  font-size: 26px;\r\n  color: #fff;\r\n}\r\n.registerForm {\r\n  margin-top: 20px;\r\n  background-color: #fff;\r\n  padding: 20px 40px 20px 20px;\r\n  border-radius: 5px;\r\n  box-shadow: 0px 5px 10px #cccc;\r\n}\r\n\r\n.submit_btn {\r\n  width: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}